  journal = {Blood},
  keywords = {Atlases as Topic,Bone Marrow Cells,Bone Marrow Cells: physiology,Cell Differentiation,Cell Differentiation: genetics,Cell Lineage,Cells, Cultured,Flow Cytometry,Gene Expression,Gene Expression Profiling,Hematopoiesis,Humans,Oligonucleotide Array Sequence Analysis,Transcription Factors,Transcription Factors: metabolism},
