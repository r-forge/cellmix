  issn = {1367-4811},
  journal = {Bioinformatics (Oxford, England)},
  keywords = {Algorithms,Biological Markers,Biological Markers: chemistry,Gene Expression Profiling,Gene Expression Profiling: methods,Models,Oligonucleotide Array Sequence Analysis,Statistical},
  month = {oct},
