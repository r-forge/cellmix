  month = {jun},
  number = {12},
  pages = {6745--50},
  pmid = {10359783},
  title = {{Broad patterns of gene expression revealed by clustering analysis of tumor and normal colon tissues probed by oligonucleotide arrays.}},
  url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=21986&tool=pmcentrez&rendertype=abstract},
